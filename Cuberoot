#include<iostream>
#include<cmath>
int main(){
	double number,cuberoot;
	std::cout<<"enter the number";
	std::cin>>number;
	if(number>=0){
		cuberoot=cbrt(number);
		std::cout<<"cuberoot of given"<<number<<"is\n"<<cuberoot<<std::endl;
	}
	else{
		std::cout<<"error or negative number"<<std::endl;
	}
	return 0;
}
